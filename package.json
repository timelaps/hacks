      "pre-commit": "npm t && npm run badges"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "jest-chain": "^1.1.2",
    "jest-coverage-badges": "^1.1.2",
